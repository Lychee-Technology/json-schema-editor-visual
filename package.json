{
  "name": "json-schema-editor-visual",
  "version": "3.0.0",
  "description": "A visual JSON Schema editor built with React, TypeScript, and Tailwind CSS",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:lib": "tsc --noEmit && vite build --mode lib",
    "preview": "pnpm run build && vite preview --outDir dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@monaco-editor/react": "^4.6.0",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "monaco-editor": "^0.55.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "pnpm@8.15.9"
}
